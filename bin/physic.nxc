// Verschiedene Modelle verkraften

float physic_rotate(float degree)
{
return (physic_pi*physic_distanceWeel/360.0*degree)/(physic_weelSize*physic_pi/360.0);
}

float physic_move(float cm)
{
return cm/(physic_weelSize*physic_pi/360.0);
}

float physic_fieldMove()
{
return (physic_weelCenterToLightSensor)/(physic_weelSize*physic_pi/360.0);
}
